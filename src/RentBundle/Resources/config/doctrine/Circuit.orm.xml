<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RentBundle\Entity\Circuit" table="circuit">
    <id name="idC" type="integer" column="id_C">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="integer" column="name" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="begin" type="string" column="begin" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="end" type="string" column="end" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pause" type="string" column="pause" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="distance" type="float" column="distance" precision="10" scale="0" nullable="false"/>
    <field name="duration" type="integer" column="duration" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="difficulty" type="string" column="difficulty" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

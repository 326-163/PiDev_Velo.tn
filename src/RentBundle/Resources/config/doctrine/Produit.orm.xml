<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RentBundle\Entity\Produit" table="produit">
    <id name="idP" type="integer" column="id_P">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomP" type="string" column="nom_P" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="typeP" type="string" column="type_P" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="marqueP" type="string" column="marque_P" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categorieP" type="string" column="categorie_P" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="couleurP" type="string" column="couleur_P" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photoP" type="string" column="photo_P" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
